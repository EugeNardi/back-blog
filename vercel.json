{
    "builds": [{
        "src": "/index.js",
         "use": "@vercel/node"
    }],
    "routes":[{
        "src": "/(.*)",
        "dest": "/"
      
    }],
    "routes":[{
    "src": "/post",
    "methods": ["GET", "POST"],
    "headers": ["Origin", "X-Requested-With", "Content-Type", "Accept"],
    "allow": ["http://localhost:5173"]
    }]
    }